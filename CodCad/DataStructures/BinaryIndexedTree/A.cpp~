#include <bits/stdc++.h>

using namespace std;

#define LSOne(S) (S & (-S))

class FenwickTree {
private:
  vector<int> ft;

public:
  FenwickTree() {}
  
  FenwickTree(int n) 
  { 
  	ft.assign(n + 1, 0); 
  }

  int rsq(int b) 
  {
  	int sum = 0; 
	for (; b; b -= LSOne(b)) 
		sum += ft[b];
    	return sum; 
  }

  int rsq(int a, int b)
  {
  	return rsq(b) - (a == 1 ? 0 : rsq(a - 1)); 
  }

  void adjust(int idx, int val) 
  { 
  	for (; idx < (int)ft.size(); idx += LSOne(idx)) 
		ft[idx] += val; 
  }
};

int main()
{



	
	return 0;
}
